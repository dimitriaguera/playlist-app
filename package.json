{
  "name": "playlist-app",
  "version": "0.0.1",
  "description": "webapp to make and read music playlist's express/react/redux/semanticUI with auth system",
  "main": "modules/core/client/index.js",
  "repository": "https://github.com/dimitriaguera/playlist-app.git",
  "author": "Dimitri Aguera <dimitri.aguera@gmail.com>",
  "scripts": {
    "test": "mocha",
    "start": "set NODE_ENV=development && node server",
    "watch": "webpack -w --config webpack.dev.js",
    "start-prod": "set NODE_ENV=production && node server",
    "build-prod": "webpack --config webpack.prod.js"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ]
          }
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "keywords": [
    "react",
    "express"
  ],
  "license": "GPL-3.0",
  "dependencies": {
    "async": "^2.5.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "chalk": "^2.2.0",
    "compression": "^1.7.1",
    "dateformat": "^3.0.2",
    "express": "^4.16.2",
    "file-type": "^6.2.0",
    "helmet": "^3.9.0",
    "immutability-helper": "^2.4.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.12.3",
    "morgan": "^1.9.0",
    "nouislider": "^10.1.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "path": "^0.12.7",
    "prop-types": "^15.6.0",
    "pug": "^2.0.0-rc.4",
    "react": "^15.6.2",
    "react-audio-player": "^0.6.2",
    "react-dom": "^15.6.1",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "read-chunk": "^2.1.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sanitize-filename": "^1.6.1",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.73.1",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "img-loader": "^2.0.0",
    "import-glob": "^1.5.0",
    "mocha": "^3.5.3",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss": "^6.0.12",
    "postcss-loader": "^2.0.6",
    "pug-loader": "^2.3.0",
    "sass-loader": "^6.0.6",
    "should": "^13.1.0",
    "style-loader": "^0.18.2",
    "stylelint": "^8.1.1",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "supertest": "^3.0.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-merge": "^4.1.0"
  }
